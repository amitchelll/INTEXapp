            <th>Age</th>
            <th>Gender</th>
            <th>Relationship_status</th>
            <th>Occupation_status</th>
            <th>Organization_ID</th>
            <th>Location</th>
            <th>Social_media</th>
            <th>Avg_time_spent</th>
        </tr>
    </thead>
    <tbody>
        <!-- You can add data rows here if needed -->
    </tbody>
</table>

